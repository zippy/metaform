<%@metaform_include_assets = true%>
<h1><%= h @form.name %> record # <%= h @record.id %></h1>

<%= error_messages_for :record %>
<% @form_html = @record.build_html(@presentation)%>
<% if @tabs%>
<%= @record.build_tabs(@tabs,@presentation)%>
<p>
<% end %>
<% form_tag(Record.url(@record.id,@presentation,@tabs), :method => :put, :id => 'metaForm') do -%>
	<%= @form_html %>
<% end -%>


